<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	http://code.alibabatech.com/schema/dubbo  http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="downdata_demo"/>
    <!-- 使用multicast广播注册中心暴露服务地址 -->
    <dubbo:consumer check="false" timeout="60000"/>

    <dubbo:registry address="zookeeper://127.0.0.1:2181"/>
    <dubbo:reference id="downdataHandler"
                     interface="com.yuntai.hdp.access.service.DowndataHandler"/>

    <dubbo:reference id="accessHospitalHandler"
                     interface="com.yuntai.hdp.access.service.AccessHospitalHandler"/>

</beans>