                   ref="userServiceImpl" cluster="failsafe" loadbalance="random" />-->

    <dubbo:service interface="dlt.domain.model.service.UserService"
                   ref="userServiceImpl" />

    <bean id="userServiceImpl" class="dlt.domain.module.service.impl.UserServiceImpl" />



</beans>